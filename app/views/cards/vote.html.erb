<%= render 'shared/toolbar' %>
<div class="internal-page">

  <div class="row clearfix">
    <div class="col-sm-4 tag-form">
      <form role="form" id="tag-search" action="">
        <input type="text" id="ts-tags" class="form-control ts-input" placeholder="funny">
      </form>
    </div>
  </div>


  <% if current_user %>
    <div class="row clearfix">

      <div class="clearfix" id="swiper">
      </div>

      <div class="col-sm-4 fullscreen">
        <span class="expand-btn glyphicon glyphicon-chevron-down"></span>
      </div>
    </div>

  <% end %>
</div>
